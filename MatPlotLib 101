        "## XKCD Style :)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "1TiGS0qZh9gF",
        "outputId": "ac6898f9-91cf-4584-cd95-bffd08865d7b"
      },
      "outputs": [
        {
          "data": {
            "text/plain": [
              "Text(0, 0.5, 'my overall health')"
            ]
          },
          "execution_count": 8,
          "metadata": {},
          "output_type": "execute_result"
        },
        {
          "data": {
            "image/png": "\n",
            "text/plain": [
              "<Figure size 432x288 with 1 Axes>"
            ]
          },
          "metadata": {},
          "output_type": "display_data"
        }
      ],
      "source": [
        "plt.xkcd()\n",
        "\n",
        "fig = plt.figure()\n",
        "ax = fig.add_subplot(1, 1, 1)\n",
        "ax.spines['right'].set_color('none')\n",
        "ax.spines['top'].set_color('none')\n",
        "plt.xticks([])\n",
        "plt.yticks([])\n",
        "ax.set_ylim([-30, 10])\n",
        "\n",
        "data = np.ones(100)\n",
        "data[70:] -= np.arange(30)\n",
        "\n",
        "plt.annotate(\n",
        "    'THE DAY I REALIZED\\nI COULD COOK BACON\\nWHENEVER I WANTED',\n",
        "    xy=(70, 1), arrowprops=dict(arrowstyle='->'), xytext=(15, -10))\n",
        "\n",
        "plt.plot(data)\n",
        "\n",
        "plt.xlabel('time')\n",
        "plt.ylabel('my overall health')"
      ]
    },
