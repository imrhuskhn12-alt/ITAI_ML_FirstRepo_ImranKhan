        }
      ],
      "source": [
        "# Remove XKCD mode:\n",
        "plt.rcdefaults()\n",
        "\n",
        "values = [12, 55, 4, 32, 14]\n",
        "colors = ['r', 'g', 'b', 'c', 'm']\n",
        "explode = [0, 0, 0.2, 0, 0]\n",
        "labels = ['India', 'United States', 'Russia', 'China', 'Europe']\n",
        "plt.pie(values, colors= colors, labels=labels, explode = explode)\n",
        "plt.title('Student Locations')\n",
        "plt.show()"
      ]
