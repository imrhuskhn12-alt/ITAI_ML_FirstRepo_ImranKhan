    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x = np.arange(1, 11)\n",
        "y = x ** 3\n",
        "\n",
        "plt.plot(x, y, marker='s', color='green')\n",
        "plt.xlabel(\"Numbers 1 to 10\")\n",
        "plt.ylabel(\"Cube of each number\")\n",
        "plt.title(\"Cubes of Numbers from 1 to 10\")\n",
        "plt.show()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 472
        },
        "id": "_Hd8K2GP0kHM",
        "outputId": "045a12d5-dd8d-4655-daad-d21bf59cd032"
      },
      "execution_count": 6,
