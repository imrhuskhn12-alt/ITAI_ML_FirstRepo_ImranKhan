        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Fake exam scores with a couple of low/high outliers\n",
        "scores_main = np.random.randint(60, 95, size=80)\n",
        "low_outliers = np.array([40, 45])\n",
        "high_outliers = np.array([98, 100])\n",
        "\n",
        "scores = np.concatenate((scores_main, low_outliers, high_outliers))\n",
        "\n",
        "plt.boxplot(scores)\n",
        "plt.title(\"Exam Scores Distribution\")\n",
